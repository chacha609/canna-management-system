{
  "name": "cannabis-cultivation-management",
  "version": "1.0.0",
  "description": "Comprehensive Cannabis Cultivation Management System with Growlink, METRC, and multi-system integrations",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint server client/src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint server client/src --ext .js,.jsx,.ts,.tsx --fix",
    "db:migrate": "knex migrate:latest --knexfile server/config/knexfile.js",
    "db:seed": "knex seed:run --knexfile server/config/knexfile.js",
    "db:rollback": "knex migrate:rollback --knexfile server/config/knexfile.js"
  },
  "keywords": [
    "cannabis",
    "cultivation",
    "management",
    "compliance",
    "METRC",
    "growlink",
    "agriculture",
    "tracking"
  ],
  "author": "Cannabis Management Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "pdf-lib": "^1.17.1",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "sharp": "^0.32.5",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.7",
    "twilio": "^4.15.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "concurrently": "^8.2.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
